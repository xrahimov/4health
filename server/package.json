{
  "name": "shifo",
  "version": "1.0.0",
  "description": "Clinic application",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start-prod": "npm run build && node ./build/index.js",
    "build": "npm run clean && npm run build-babel",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "start-dev": "node --exec babel-node src/index.js",
    "server": "nodemon --exec babel-node src/index.js",
    "client": "npm start --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xrahimov/shifo.git"
  },
  "keywords": [
    "clinic",
    "mern"
  ],
  "author": "@sjamoladdin and @xrahimov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xrahimov/shifo/issues"
  },
  "homepage": "https://github.com/xrahimov/shifo#readme",
  "dependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.13.16",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.26.0",
    "concurrently": "^6.2.0",
    "config": "^3.3.6",
    "connect-flash": "^0.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-list-endpoints": "^5.0.0",
    "faker": "^5.5.3",
    "html-pdf": "^3.0.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.11",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "socket.io": "^4.1.3"
  }
}
